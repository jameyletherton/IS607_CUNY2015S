CREATE TABLE Instructor_T
(
Instructor_ID int PRIMARY KEY,
Instructor_Name varchar NOT NULL,
Department varchar NOT NULL,
Title varchar NULL,
Hired date NOT NULL,
Terminated date NULL,
Suspended date NULL,
Suspended_Reason varchar NULL,
ACTIVE varchar NOT NULL
);

