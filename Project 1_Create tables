CREATE TABLE TestScore_T
(
Test_ID varchar NOT NULL,
Score int NOT NULL,
Instructor_ID int NOT NULL REFERENCES Instructor_T,
Student_ID int NOT NULL REFERENCES Student_T,
Insert date NOT NULL,
Update_Reason varchar NULL,
Update date NULL
);

